{
  "name": "majidhajian",
  "description": "Majid Hajian official website",
  "version": "1.1.0",
  "author": "Majid Hajian ( mhadaily@gmail.com )",
  "keywords": [
    "gatsby, typescript",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby clean && gatsby build",
    "start": "gatsby clean && gatsby develop",
    "deploy:gh": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy:netlify": "gatsby build --prefix-paths && netlify deploy --prod --dir=public",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "lint:ts": "tslint --fix 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "lint:style": "stylelint 'src/**/*.{scss,ts,css,sass,less,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,md}'",
    "formatAndFix": "npm run format && npm run lint:ts && npm run style"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^5.0.1",
    "@types/typography": "^0.16.3",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby-cli": "^2.11.7",
    "gatsby-plugin-catch-links": "^2.2.1",
    "gatsby-plugin-lodash": "^3.2.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-netlify": "^2.2.1",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-plugin-typography": "^2.4.1",
    "gatsby-remark-autolink-headers": "^2.2.1",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.4",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "react-typography": "^0.16.19",
    "source-map-support": "latest",
    "stylelint": "^13.3.2",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.16.1",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^4.2.0",
    "tslint-react-a11y": "^1.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "gatsby": "^2.20.18",
    "gatsby-image": "^2.3.2",
