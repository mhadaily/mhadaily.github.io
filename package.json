{
  "name": "majidhajian",
  "description": "Majid Hajian official website",
  "version": "1.1.0",
  "author": "Majid Hajian ( mhadaily@gmail.com )",
  "keywords": [
    "gatsby, typescript",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby clean && gatsby build",
    "start": "gatsby clean && gatsby develop",
    "deploy:gh": "gatsby build --prefix-paths && gh-pages -d public -b master",
    "deploy:netlify": "gatsby build --prefix-paths && netlify deploy --prod --dir=public",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "lint:ts": "tslint --fix 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "lint:style": "stylelint 'src/**/*.{scss,ts,css,sass,less,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,md}'",
    "formatAndFix": "npm run format && npm run lint:ts && npm run style"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.159",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^5.0.16",
    "@types/styled-components": "^5.1.2",
    "@types/typography": "^0.16.3",
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby-cli": "^2.12.91",
    "gatsby-plugin-catch-links": "^2.3.11",
    "gatsby-plugin-lodash": "^3.3.10",
    "gatsby-plugin-manifest": "^2.4.23",
    "gatsby-plugin-netlify": "^2.3.13",
    "gatsby-plugin-offline": "^3.2.26",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-typescript": "^2.4.18",
    "gatsby-plugin-typography": "^2.5.10",
    "gatsby-remark-autolink-headers": "^2.3.11",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-prismjs": "^3.5.10",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-remark": "^2.8.28",
    "gatsby-transformer-sharp": "^2.5.13",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-typography": "^0.16.19",
    "source-map-support": "^0.5.19",
    "stylelint": "^13.6.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^4.2.0",
    "tslint-react-a11y": "^1.1.0",
    "typescript": "^3.9.7"
